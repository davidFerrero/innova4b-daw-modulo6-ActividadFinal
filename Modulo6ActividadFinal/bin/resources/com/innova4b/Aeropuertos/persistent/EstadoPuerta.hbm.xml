<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.innova4b.Aeropuertos.persistent">
	<class name="EstadoPuerta" table="EstadoPuerta">
		<id name="idEstado" column="idEstado">
			<generator class="increment"></generator>
		</id>
		<property name="descripcion"></property>
		<property name="disponible"></property>
	</class>	
</hibernate-mapping>